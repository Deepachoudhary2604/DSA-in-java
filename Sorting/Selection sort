class Solution {
    void selectionSort(int[] arr) {
        for(int i=0;i<arr.length;i++){
            int mini=i;
            for(int j=i+1;j<arr.length;j++){
                if(arr[mini]>arr[j]){
                    mini=j;
                }
            }
            int temp=arr[mini];
            arr[mini]=arr[i];
            arr[i]=temp;
        }
    }
}
