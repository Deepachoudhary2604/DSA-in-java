class Solution {
    public void rotateclockwise(int[] arr, int k) {
        if(arr.length<=1){
            return;
        }
        int n=arr.length;
        k=k%n;
        Reverse(arr,0,arr.length-1);
        Reverse(arr,0,k-1);
        Reverse(arr,k,arr.length-1);
    }
    public void Reverse(int[] arr,int s,int e){
        while(s<=e){
            int temp=arr[s];
            arr[s]=arr[e];
            arr[e]=temp;
            s++;
            e--;
        }
    }
}
