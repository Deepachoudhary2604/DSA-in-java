class Solution {
    public int getSecondLargest(int[] arr) {
        int max1=Integer.MIN_VALUE;
        int max2=Integer.MAX_VALUE;
        for(int i=0;i<arr.length;i++){
            if(max1<arr[i]){
                max2=max1;
                max1=arr[i];
            }
            else if(max2<arr[i]){
                max2=arr[i];
            }
        }
        return max2;
    }
}
